    RowBox[{"Table", "[", 
     RowBox[{
      RowBox[{"Sort", "@", 
       RowBox[{"Chop", "@", 
        RowBox[{"Eigenvalues", "[", 
         RowBox[{"H", "/.", 
          RowBox[{"k", "->", 
           RowBox[{
           "klist", "\[LeftDoubleBracket]", "i", 
            "\[RightDoubleBracket]"}]}]}], "]"}]}]}], ",", 
      RowBox[{"{", 
       RowBox[{"i", ",", 
        RowBox[{"Length", "[", "klist", "]"}]}], "}"}]}], "]"}], "]"}]}], 
  ";"}]], "Input",
 CellChangeTimes->{{3.8779501779181967`*^9, 3.8779501801063213`*^9}},

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"Esx", "[", 
     RowBox[{
     "x1_", ",", "y1_", ",", "z1_", ",", "x2_", ",", "y2_", ",", "z2_", ",", 
      "nvec_", ",", "sp\[Sigma]_"}], "]"}], ":=", 
    RowBox[{"Block", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
        RowBox[{"v", "=", 
         RowBox[{
          RowBox[{"{", 
           RowBox[{"x2", ",", "y2", ",", "z2"}], "}"}], "-", 
          RowBox[{"{", 
           RowBox[{"x1", ",", "y1", ",", "z1"}], "}"}]}]}], ",", 
        "cos\[Theta]"}], "}"}], ",", 
      RowBox[{
       RowBox[{"cos\[Theta]", "=", 
        FractionBox[
         RowBox[{"v", ".", "nvec"}], 
         SqrtBox[
          RowBox[{"v", ".", "v"}]]]}], ";", 
       RowBox[{"cos\[Theta]", " ", "sp\[Sigma]"}]}]}], "]"}]}], ";"}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"Exx", "[", 
     RowBox[{
     "x1_", ",", "y1_", ",", "z1_", ",", "x2_", ",", "y2_", ",", "z2_", ",", 
      "nvec_", ",", "pp\[Sigma]_", ",", "pp\[Pi]_"}], "]"}], ":=", 
    RowBox[{"Block", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
        RowBox[{"v", "=", 
         RowBox[{
          RowBox[{"{", 
           RowBox[{"x2", ",", "y2", ",", "z2"}], "}"}], "-", 
          RowBox[{"{", 
           RowBox[{"x1", ",", "y1", ",", "z1"}], "}"}]}]}], ",", 
        "cos\[Theta]"}], "}"}], ",", 
      RowBox[{
       RowBox[{"cos\[Theta]", "=", 
        FractionBox[
         RowBox[{"v", ".", "nvec"}], 
         SqrtBox[
          RowBox[{"v", ".", "v"}]]]}], ";", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{
         SuperscriptBox["cos\[Theta]", "2"], " ", "pp\[Sigma]"}], "+", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"1", "-", 
           SuperscriptBox["cos\[Theta]", "2"]}], ")"}], " ", 
         "pp\[Pi]"}]}]}]}], "]"}]}], ";"}], "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"Exy", "[", 
    RowBox[{
    "x1_", ",", "y1_", ",", "z1_", ",", "x2_", ",", "y2_", ",", "z2_", ",", 
     "nvec1_", ",", "nvec2_", ",", "pp\[Sigma]_", ",", "pp\[Pi]_"}], "]"}], ":=", 
   RowBox[{"Block", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"v", "=", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{"x2", ",", "y2", ",", "z2"}], "}"}], "-", 
         RowBox[{"{", 
          RowBox[{"x1", ",", "y1", ",", "z1"}], "}"}]}]}], ",", 
       "cos\[Theta]1", ",", "cos\[Theta]2"}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"cos\[Theta]1", "=", 
       FractionBox[
        RowBox[{"v", ".", "nvec1"}], 
        SqrtBox[
         RowBox[{"v", ".", "v"}]]]}], ";", "\[IndentingNewLine]", 
      RowBox[{"cos\[Theta]2", "=", 
       FractionBox[
        RowBox[{"v", ".", "nvec2"}], 
        SqrtBox[
         RowBox[{"v", ".", "v"}]]]}], ";", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"pp\[Sigma]", "-", "pp\[Pi]"}], ")"}], " ", "cos\[Theta]1", " ",
        "cos\[Theta]2"}]}]}], "]"}]}], ";"}]}], "Code",
 CellChangeTimes->{

 CellID->3602077,ExpressionUUID->"8d5ae09d-0cff-4a86-9f7c-887cc2918661"],

Cell[TextData[{
 "Now we can solve the generalized eigenvalue problem for the constructed \
Hamiltonian and the matrix of atomic orbitals overlaps for a range of ",
 Cell[BoxData[
  StyleBox["k", "TI"]], "InlineFormula",ExpressionUUID->
  "9a7f54da-628b-470c-9a78-391fc5500077"],
 "-points. "
}], "Text",
 CellChangeTimes->{{3.8780293634326067`*^9, 3.8780293973285456`*^9}},
 CellID->250953536,ExpressionUUID->"ed2ba1db-52f7-43b0-9174-9c7dc24b2e5a"],

Cell[CellGroupData[{

Cell[TextData[{
 "The first Brillouin zone is sampled with ",
 Cell[BoxData[
  StyleBox["k", "TI"]], "InlineFormula",ExpressionUUID->
  "c7df1d2f-5353-4426-bd7c-70ddcafef69e"],
 "-points only along the lines connecting the high symmetry points K-\
\[CapitalGamma]-M-K:"
}], "Text",
 CellChangeTimes->{{3.8780989127606955`*^9, 3.87809901477853*^9}},
 CellID->100137321,ExpressionUUID->"4b44e77e-b377-4960-868d-a921636964df"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"\[CapitalGamma]", "=", 
   RowBox[{"{", 
    RowBox[{"0", ",", "0", ",", "0"}], "}"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"K", "=", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"4", 
      RowBox[{"\[Pi]", "/", 
       RowBox[{"(", 
        RowBox[{"3", "a"}], ")"}]}]}], ",", "0", ",", "0"}], "}"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"M", "=", 
   RowBox[{"{", 
    RowBox[{
     FractionBox["\[Pi]", "a"], ",", 
     RowBox[{"-", 
      FractionBox["\[Pi]", 
       RowBox[{
        SqrtBox["3"], "a"}]]}], ",", "0"}], "}"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"np", "=", "20"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"klist", "=", 
   RowBox[{"Join", "[", 
    RowBox[{
     RowBox[{"Table", "[", 
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{"(", 
          RowBox[{"\[CapitalGamma]", "-", "K"}], ")"}], 
         FractionBox["i", "np"]}], "+", "K"}], ",", 
       RowBox[{"{", 
        RowBox[{"i", ",", "np"}], "}"}]}], "]"}], ",", 
     RowBox[{"Table", "[", 
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{"(", 
          RowBox[{"M", "-", "\[CapitalGamma]"}], ")"}], 
         FractionBox["i", "np"]}], "+", "\[CapitalGamma]"}], ",", 
       RowBox[{"{", 
        RowBox[{"i", ",", "np"}], "}"}]}], "]"}], ",", 
     RowBox[{"Table", "[", 
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{"(", 
          RowBox[{"K", "-", "M"}], ")"}], 
         FractionBox["i", "np"]}], "+", "M"}], ",", 
       RowBox[{"{", 
        RowBox[{"i", ",", "np"}], "}"}]}], "]"}]}], "]"}]}], ";"}]}], "Input",
 CellChangeTimes->{3.8780294274342675`*^9},
 CellLabel->"In[67]:=",
 CellID->53672,ExpressionUUID->"7e320483-d6b9-4d90-8abe-6f74d07bb87b"]
}, Open  ]],

Cell[CellGroupData[{

Cell[TextData[{
 "The energy bands along the sampled lines can be found with ",
 Cell[BoxData[
  ButtonBox["Eigenvalues",
   BaseStyle->"Link"]], "InlineFormula",ExpressionUUID->
  "5f817d91-af03-453c-b436-6bf49c2c9a04"],
 " and sorted with ",
 Cell[BoxData[
  ButtonBox["Sort",
   BaseStyle->"Link"]], "InlineFormula",ExpressionUUID->
  "21d99e64-f489-428c-9b14-5377ed28d69c"],
 ":"
}], "Text",
 CellChangeTimes->{{3.8780990415610623`*^9, 3.878099078025148*^9}},
 CellID->230373590,ExpressionUUID->"48698e0f-67b4-4301-b242-4dc50eb39f7e"],

Cell[BoxData[
 RowBox[{
  RowBox[{"bands", "=", 
   RowBox[{"Table", "[", 
    RowBox[{
     RowBox[{"Sort", "@", 
      RowBox[{"Chop", "@", 
       RowBox[{"Eigenvalues", "[", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{"H", ",", "S"}], "}"}], "/.", 
         RowBox[{"k", "->", 
          RowBox[{
          "klist", "\[LeftDoubleBracket]", "i", "\[RightDoubleBracket]"}]}]}],
         "]"}]}]}], ",", 
     RowBox[{"{", 
      RowBox[{"i", ",", 
       RowBox[{"Length", "[", "klist", "]"}]}], "}"}]}], "]"}]}], 
  ";"}]], "Input",

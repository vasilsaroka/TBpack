Cell[TextData[{
 "Next we need to solve the eigenvalue problem for the constructed \
Hamiltonian for a range of ",
 Cell[BoxData[
  StyleBox["k", "TI"]], "InlineFormula",ExpressionUUID->
  "bfb94403-b8c5-4b45-ba03-fe53dab820c2"],
 "-points. "
}], "Text",
 CellChangeTimes->{{3.8779498562137957`*^9, 3.877950028722663*^9}, {
  3.877950059927448*^9, 3.8779500879830523`*^9}},
 CellID->428104919,ExpressionUUID->"34ea9d49-9d42-419d-a652-c2597d194b37"],

Cell[CellGroupData[{

Cell["\<\
To do this, we sample the path through the high symmetry \
M-\[CapitalGamma]-K-M of the Brillouin zone of monolayer GaAs (2D honeycomb \
lattice):\
\>", "Text",
 CellChangeTimes->{{3.8779498562137957`*^9, 3.877950028722663*^9}, {
  3.877950875118074*^9, 3.8779509442990313`*^9}},
 CellID->72159388,ExpressionUUID->"39ea8618-7d5b-400c-8c08-dc28d2e4937d"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"\[CapitalGamma]", "=", 
   RowBox[{"{", 
    RowBox[{"0", ",", "0", ",", "0"}], "}"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"K", "=", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"4", 
      RowBox[{"\[Pi]", "/", 
       RowBox[{"(", 
        RowBox[{"3", "a"}], ")"}]}]}], ",", "0", ",", "0"}], "}"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"M", "=", 
   RowBox[{"{", 
    RowBox[{
     FractionBox["\[Pi]", "a"], ",", 
     RowBox[{"-", 
      FractionBox["\[Pi]", 
       RowBox[{
        SqrtBox["3"], "a"}]]}], ",", "0"}], "}"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"np", "=", "40"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"klist", "=", 
   RowBox[{"Join", "[", 
    RowBox[{
     RowBox[{"Table", "[", 
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{"(", 
          RowBox[{"\[CapitalGamma]", "-", "M"}], ")"}], 
         FractionBox["i", "np"]}], "+", "M"}], ",", 
       RowBox[{"{", 
        RowBox[{"i", ",", "np"}], "}"}]}], "]"}], ",", 
     RowBox[{"Table", "[", 
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{"(", 
          RowBox[{"K", "-", "\[CapitalGamma]"}], ")"}], 
         FractionBox["i", "np"]}], "+", "\[CapitalGamma]"}], ",", 
       RowBox[{"{", 
        RowBox[{"i", ",", "np"}], "}"}]}], "]"}], ",", 
     RowBox[{"Table", "[", 
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{"(", 
          RowBox[{"M", "-", "K"}], ")"}], 
         FractionBox["i", "np"]}], "+", "K"}], ",", 
       RowBox[{"{", 
        RowBox[{"i", ",", "np"}], "}"}]}], "]"}]}], "]"}]}], ";"}]}], "Input",
 CellChangeTimes->{{3.8779500493408422`*^9, 3.877950056221236*^9}},
 CellLabel->"In[105]:=",
 CellID->130080763,ExpressionUUID->"ac18a8d1-84a9-4338-987a-4ad394ea02e8"]
}, Open  ]],

Cell[CellGroupData[{

Cell[TextData[{
 "and solve the problem with ",
 Cell[BoxData[
  ButtonBox["Eigenvalues",
   BaseStyle->"Link"]], "InlineFormula",ExpressionUUID->
  "8f2a4666-61b3-4c8a-8705-8c7d2d689121"],
 " :"
}], "Text",
